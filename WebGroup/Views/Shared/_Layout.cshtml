<body>
    <!--====== Preloader Area Start ======-->
    <div id="preloader">
        <!-- Digimax Preloader -->
        <div id="digimax-preloader" class="digimax-preloader">
            <!-- Preloader Animation -->
            <div class="preloader-animation">
                <!-- Spinner -->
                <div class="spinner"></div>
                <!-- Loader -->
                <div class="loader">
                    <span data-text-preloader="С" class="animated-letters">С</span>
                    <span data-text-preloader="И" class="animated-letters">И</span>
                    <span data-text-preloader="П" class="animated-letters">П</span>
                    <span data-text-preloader="-" class="animated-letters">-</span>
                    <span data-text-preloader="3" class="animated-letters">3</span>
                    <span data-text-preloader="1" class="animated-letters">1</span>
                    <span data-text-preloader="3" class="animated-letters">3</span>
                </div>
                <p class="fw-5 text-center text-uppercase">Loading</p>
            </div>
            <!-- Loader Animation -->
            <div class="loader-animation">
                <div class="row h-100">
                    <!-- Single Loader -->
                    <div class="col-3 single-loader p-0">
                        <div class="loader-bg"></div>
                    </div>
                    <!-- Single Loader -->
                    <div class="col-3 single-loader p-0">
