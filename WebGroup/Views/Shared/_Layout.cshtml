                                <ul>
                                    <li class="py-2"><a class="text-black-50" asp-action="About">О группе</a></li>
                                    <li class="py-2"><a class="text-black-50" asp-action="AboutCollege">О колледже</a></li>
                                    <li class="py-2"><a class="text-black-50" asp-action="Project">Проекты</a></li>
                                    <li class="py-2"><a class="text-black-50" asp-action="Achievement">Достижения</a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-12 col-sm-6 col-lg-3">
                            <!-- Footer Items -->
                            <div class="footer-items">
                                <!-- Footer Title -->
                                <h3 class="footer-title text-uppercase mb-2">Работы</h3>
                                <ul>
                                    <li class="py-2"><a class="text-black-50" asp-action="Project">Курсовые</a></li>
                                    <li class="py-2"><a class="text-black-50" asp-action="News">Новости</a></li>
                                    <li class="py-2"><a class="text-black-50" asp-action="Project">Дипломные</a></li>
                                    <li class="py-2"><a class="text-black-50" asp-action="Project">Прочее</a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-12 col-sm-6 col-lg-3">
                            <!-- Footer Items -->
                            <div class="footer-items">
                                <!-- Footer Title -->
                                <h3 class="footer-title text-uppercase mb-2">Поддержка</h3>
                                <ul>
                                    <li class="py-2"><a class="text-black-50" href="#">Написать</a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-12 col-sm-6 col-lg-3">
                            <!-- Footer Items -->
                            <div class="footer-items">
                                <!-- Footer Title -->
                                <h3 class="footer-title text-uppercase mb-2">Мы в соцсетях</h3>
                                <p class="mb-2">Ищи нас в соц сетях</p>
                                <!-- Social Icons -->
                                <ul class="social-icons list-inline pt-2">
                                    <li class="list-inline-item px-1"><a href="https://telegram.org/"><i class="fab fa-telegram"></i></a></li>
                                    <li class="list-inline-item px-1"><a href="https://www.youtube.com/"><i class="fab fa-youtube"></i></a></li>
                                    <li class="list-inline-item px-1"><a href="https://www.whatsapp.com/"><i class="fab fa-whatsapp"></i></a></li>
                                    <li class="list-inline-item px-1"><a href="http://vk.com/"><i class="fab fa-vk"></i></a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Footer Bottom -->
